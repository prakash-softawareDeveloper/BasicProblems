import java.util.*;
public class Main {
    public static int findFibonacci(int A) {
        if(A == 0 || A == 1){
            return A;
        }
        int ans = findFibonacci(A - 1) + findFibonacci(A - 2);
        return ans;
    }
    public static void main(String args[]) {
     int a=10;
     int ans=findFibonacci(a);
     System.out.println(ans);
    }
}